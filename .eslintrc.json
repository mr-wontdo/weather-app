{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "consistent-return":"off",
        "indent": "off",
        "max-len":"off",
        "no-alert":"off",
        "no-console": "off",
        "no-multiple-empty-lines":"off",
        "no-prototype-builtins":"off",
        "no-unused-vars":"off",
        "no-use-before-define":"off",
        "prefer-destructuring":"off",
        "prefer-template":"off"
    }
}
